pipeline {
    agent any 
    stages {
        stage('cloning') { 
            steps {
               echo "cloning"
                git 'https://github.com/said1024/jenkins3.git'
            }
        }
 stage('maven build') { 
            steps {
               echo "build"
                sh "mvn clean package"
            }
        }
 stage('Test') {
            steps {
                echo "unit tests"
                sh "mvn test"
            }
        }
       
    }
}
